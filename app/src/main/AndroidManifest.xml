<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nissen.johannes.fremtidsmentor">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_output_onlinepngtools_11"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_rounded"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".screenshots.ActivityCommunity" android:screenOrientation="portrait"></activity>
        <activity android:name=".screenshots.ActivityMain" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>